{
  "name": "react-redux-seed",
  "version": "0.1.0",
  "description": "A boiler-plate setup for React/Redux applications",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --progress",
    "clean": "rimraf dist node_modules",
    "build": "NODE_ENV=production webpack -p",
    "cover": "istanbul cover _mocha -- --opts ./mocha.opts src/**/*.test.js",
    "test": "mocha --opts ./mocha.opts src/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint ."
  },
  "keywords": [
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/react-redux-seed.git"
  },
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.7.0",
    "babel": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-plugin-transform-decorators": "^6.1.18",
    "chai": "^3.4.0",
    "css-loader": "^0.21.0",
    "eslint": "^1.8.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.6.3",
    "file-loader": "^0.8.4",
    "history": "^1.13.0",
    "immutable": "^3.7.5",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.2",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "radium": "^0.14.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.4",
    "redux-actions": "^0.8.0",
    "redux-devtools": "^2.1.0",
    "redux-logger": "^2.0.4",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.4.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
